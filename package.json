{
    "name": "lru-collection",
    "version": "0.1.1",
    "author": {
        "name": "Maston",
        "url": "https://github.com/mastondzn"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/mastondzn/lru-collection"
    },
    "keywords": [
        "lru",
        "collection",
        "cache",
        "lrucache",
        "lru-cache"
    ],
    "description": "In memory LRU cache with more utility methods.",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "module": "dist/index.mjs",
    "exports": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.mjs",
        "require": "./dist/index.js"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "test": "jest",
        "test:watch": "jest --watch",
        "build": "tsup",
        "docs": "typedoc src/index.ts --out docs --excludePrivate --excludeProtected",
        "lint": "eslint .",
        "typecheck": "tsc --noEmit",
        "preinstall": "npx only-allow pnpm"
    },
    "devDependencies": {
        "@types/jest": "^29.5.1",
        "@types/node": "18",
        "@typescript-eslint/eslint-plugin": "^5.59.2",
        "@typescript-eslint/parser": "^5.59.2",
        "eslint": "^8.40.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-define-config": "^1.20.0",
        "eslint-plugin-unicorn": "^47.0.0",
        "jest": "^29.5.0",
        "only-allow": "^1.1.1",
        "prettier": "^2.8.8",
        "ts-jest": "^29.1.0",
        "tsup": "^6.7.0",
        "typedoc": "^0.24.6",
        "typescript": "^5.0.4"
    },
    "packageManager": "pnpm@8.4.0",
    "dependencies": {
        "@discordjs/collection": "^1.5.1"
    }
}
